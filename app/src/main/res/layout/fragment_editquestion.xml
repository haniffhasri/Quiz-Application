<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout13"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".editquestion">

    <EditText
        android:id="@+id/ques"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="76dp"
        android:background="@drawable/outline"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:inputType="textPersonName"
        android:padding="6dp"
        android:textColor="#FFFF"
        app:layout_constraintBottom_toTopOf="@+id/choi1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toEndOf="@+id/imageView" />

    <EditText
        android:id="@+id/choi1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/outline"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:inputType="textPersonName"
        android:padding="6dp"
        android:textColor="#FFFF"
        app:layout_constraintBottom_toTopOf="@+id/choi2"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/choi2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:textColor="#FFFF"
        android:background="@drawable/outline"
        android:padding="6dp"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/choi3"
        app:layout_constraintEnd_toEndOf="@+id/choi1"
        app:layout_constraintStart_toStartOf="@+id/choi1" />

    <EditText
        android:id="@+id/choi3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:textColor="#FFFF"
        android:background="@drawable/outline"
        android:padding="6dp"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/choi4"
        app:layout_constraintEnd_toEndOf="@+id/choi2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/choi2" />

    <EditText
        android:id="@+id/choi4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:textColor="#FFFF"
        android:background="@drawable/outline"
        android:padding="6dp"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/cor"
        app:layout_constraintEnd_toEndOf="@+id/choi3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/choi3" />

    <Spinner
        android:id="@+id/cor"
        android:layout_width="169dp"
        android:layout_height="27dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/bg_spinner"
        app:layout_constraintBottom_toTopOf="@+id/hin"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/hin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:background="@drawable/outline"
        android:ems="10"
        android:fontFamily="@font/poppins"
        android:inputType="textPersonName"
        android:padding="6dp"
        android:textColor="#FFFF"
        app:layout_constraintBottom_toTopOf="@+id/editq"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toEndOf="@+id/imageView15" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="47dp"
        android:layout_height="43dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="44dp"
        android:src="@drawable/question"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton"
        app:layout_constraintVertical_bias="0.75" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="48dp"
        android:layout_height="43dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="28dp"
        android:src="@drawable/choose"
        app:layout_constraintBottom_toTopOf="@+id/choi2"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="42dp"
        android:layout_height="33dp"
        android:layout_marginEnd="12dp"
        android:src="@drawable/correct"
        app:layout_constraintBottom_toBottomOf="@+id/cor"
        app:layout_constraintEnd_toStartOf="@+id/cor"
        app:layout_constraintTop_toTopOf="@+id/cor"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="42dp"
        android:layout_height="38dp"
        android:layout_marginStart="52dp"
        android:src="@drawable/lightbulb"
        app:layout_constraintBottom_toBottomOf="@+id/hin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hin"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/editq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="84dp"
        android:background="@drawable/round_corner"
        android:fontFamily="@font/poppins"
        android:text="confirm edit"
        android:textColor="#FFFF"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        android:src="@drawable/ic_back"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="415dp"
        android:layout_height="36dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="EDIT QUESTION"
        android:textColor="#FFFF"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="@+id/imageView30"
        app:layout_constraintTop_toTopOf="@+id/imageView30"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>